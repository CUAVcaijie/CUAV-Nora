# setup the onboard PWM number
BRD_PWM_COUNT 15

# Switch on the onboard RGB indicator
SERVO15_FUNCTION 120
NTF_LED_TYPES 455

# setup the temperature compensation
BRD_IMU_TARGTEMP -1

# turn on the CAN power monitoring(default)
CAN_P1_DRIVER 1
BATT_MONITOR 8

# setup the parameter for the ADC power module
BATT_VOLT_PIN 16
BATT_CURR_PIN 17
BATT_VOLT_MULT 18.182
BATT_AMP_PERVLT 36.364

